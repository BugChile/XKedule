{
 "manifest_version": 2,
 "name": "XKedule",
 "description": "Daily scheduler",
 "version": "0.1",
 "author": "Bug Chile",
"browser_action": {
   "default_icon": "logo-small.png",
   "default_title": "Have a good day"
 },
"chrome_url_overrides" : {
  "newtab": "index.html"
},
 "permissions": ["activeTab",
                 "storage"]
}
